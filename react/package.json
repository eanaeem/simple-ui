{
  "name": "react-simple-ui",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Sireesh Pangaluri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ineffablep/simple-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ineffablep/simple-ui/issues"
  },
  "homepage": "https://github.com/ineffablep/simple-ui/blob/master/README.md",
  "license": "MIT",
  "keywords": [
    "React JS",
    "Reactjs",
    "Material Design",
    "Material",
    "Flat Design",
    "boiler-plate",
    "WebPack",
    "Karma",
    "Jasmine",
    "Protractor",
    "Unit Testing",
    "Testing",
    "Angular",
    "Reactive",
    "Rx"
  ],
  "dependencies": {
    "object-assign": "4.1.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-loader": "6.2.9",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "browser-sync": "2.18.2",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.1",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "isparta": "4.0.0",
    "jslint": "0.10.3",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "3.2.0",
    "node-less": "1.0.0",
    "node-sass": "3.13.1",
    "npm-run-all": "3.1.2",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.1",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  }
}
